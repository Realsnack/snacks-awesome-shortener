meta {
  name: POST /short - valid extra
  type: http
  seq: 3
}

post {
  url: {{base_url}}/short
  body: json
  auth: inherit
}

body:json {
  {
    "url": "https://shorts.michalsvacina.cz/",
    "surl": "https://hltv.org/"
  }
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.long_url: neq htps://hltv.org
  res.body.long_url: eq https://shorts.michalsvacina.cz/
}

settings {
  encodeUrl: true
  timeout: 0
}
